<DialogPane prefHeight="464.0" prefWidth="971.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.fileMenu.OptionsController">
   <content>
      <GridPane alignment="BOTTOM_CENTER">
